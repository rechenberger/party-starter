{
  "name": "party-starter",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:e2e": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "e2e:install": "playwright install --with-deps chromium",
    "e2e:build": "next build",
    "e2e:start": "next start -p 3000",
    "e2e:seed": "tsx src/scripts/e2e-seed.ts",
    "e2e:test": "playwright test",
    "e2e:ci": "tsx src/scripts/e2e-run.ts --mode ci",
    "e2e:dev": "tsx src/scripts/e2e-run.ts --mode dev",
    "e2e:dev:grep": "pnpm e2e:dev -- --grep",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "ts:check": "tsc --noEmit --incremental false",
    "email:dev": "email dev --port 3001",
    "starter:init": "tsx src/scripts/init.ts",
    "postinstall": "tsx src/scripts/crons-sync.ts",
    "neon:auth": "neonctl auth",
    "neon:dev:use": "tsx src/scripts/neon-dev-branch.ts sync",
    "neon:dev:create": "tsx src/scripts/neon-dev-branch.ts create",
    "neon:dev:reset": "tsx src/scripts/neon-dev-branch.ts reset",
    "neon:dev:delete": "tsx src/scripts/neon-dev-branch.ts delete",
    "neon:dev:url": "tsx src/scripts/neon-dev-branch.ts url",
    "neon:dev:env": "tsx src/scripts/neon-dev-branch.ts env",
    "neon:dev:seed": "tsx src/scripts/neon-dev-seed.ts",
    "neon:prod:env": "tsx src/scripts/neon-dev-branch.ts env --branch production",
    "neon:dev:help": "tsx src/scripts/neon-dev-branch.ts --help"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
