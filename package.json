    "nanoid": "^5.1.6",
    "negotiator": "^1.0.0",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.13",
    "postcss": "8.5.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.4",
    "react-day-picker": "9.13.2",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4",
    "recharts": "^2.15.4",
    "remeda": "^2.33.6",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "vaul": "^1.1.2",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bcrypt": "^6.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/ms": "^2.1.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "25.2.2",
    "@types/nodemailer": "^7.0.9",
