    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.5.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "embla-carousel-react": "^8.6.0",
    "eslint-config-next": "16.1.6",
    "input-otp": "^1.4.2",
    "jotai": "^2.17.1",
    "lodash-es": "^4.17.23",
    "lucide-react": "^0.563.0",
